    "y_true = y_te.argmax(axis=1)\n",
    "y_pred = y_pred.argmax(axis=1)\n",
    "\n",
    "plot_confusion_matrix(y_true, y_pred, ['STAR','GALAXY','QSO'],\n",
    "                      1)"
